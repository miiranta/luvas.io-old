<script>

//vars
const jwt = "{{req.user.token}}"

//Start
const socket = io()

//Send jwt
socket.emit("auth", jwt)

//Websockets GET------------------------
socket.on("error",(data)=>{
    console.log("Error: " + data)
})

socket.on("message",(data)=>{
    console.log(data)
})
//--------------------------------------

//Websockets SEND------------------------



//--------------------------------------



</script>